\ProvidesFile{ext-biblatex-aux.def}
  [2017/12/01 v0.2 auxiliaries for extended biblatex citation style (MW)]

% outer cite delimiters
\newcommand*{\DeclarePairedCiteOuterDelim}[3]{%
  \csdef{exblx@delim@#1@outer@open}{#2}%
  \csdef{exblx@delim@#1@outer@close}{#3}}

\newcommand*{\DeclarePairedCiteOuterDelimAlias}{%
  \@ifstar
    {\exblx@declare@paired@cite@outer@delim@alias@i}
    {\exblx@declare@paired@cite@outer@delim@alias}}
\def\exblx@declare@paired@cite@outer@delim@alias#1#2{%
  \csdef{exblx@delim@#1@outer@open}{\csuse{exblx@delim@#2@outer@open}}%
  \csdef{exblx@delim@#1@outer@close}{\csuse{exblx@delim@#2@outer@close}}}
\def\exblx@declare@paired@cite@outer@delim@alias@i#1#2{%
  \csletcs{exblx@delim@#1@outer@open}{exblx@delim@#2@outer@open}%
  \csletcs{exblx@delim@#1@outer@close}{exblx@delim@#2@outer@close}}

\newcommand*{\exblx@create@mkpairedouterdelim}[1]{%
  \protected\csdef{mkpairedouter#1delim}##1{%
    \begingroup
      \blx@blxinit
      \blx@setsfcodes
      \csuse{exblx@delim@#1@outer@open}%
      ##1%
      \csuse{exblx@delim@#1@outer@close}%
    \endgroup}}

\exblx@create@mkpairedouterdelim{cite}
\exblx@create@mkpairedouterdelim{parencite}
\exblx@create@mkpairedouterdelim{textcite}
\exblx@create@mkpairedouterdelim{bibxrefcite}

