\ProvidesFile{ext-biblatex-aux.def}
  [2017/12/01 v0.2 auxiliaries for extended biblatex citation style (MW)]

% outer cite delimiters
\newcommand*{\DeclarePairedOuterCiteDelim}[3]{%
  \csdef{outer@#1@delim@open}{#2}%
  \csdef{outer@#1@delim@close}{#3}}

\newcommand*{\DeclarePairedOuterCiteDelimAlias}{%
  \@ifstar
    {\exblx@declarepairedoutercitedelimalias@i}
    {\exblx@declarepairedoutercitedelimalias}}
\def\exblx@declarepairedoutercitedelimalias#1#2{%
  \csdef{outer@#1@delim@open}{\csuse{outer@#2@delim@open}}%
  \csdef{outer@#1@delim@close}{\csuse{outer@#2@delim@close}}}
\def\exblx@declarepairedoutercitedelimalias@i#1#2{%
  \csletcs{outer@#1@delim@open}{outer@#2@delim@open}%
  \csletcs{outer@#1@delim@close}{outer@#2@delim@close}}

\newcommand*{\blx@createmkpairedouterdelim}[1]{%
  \protected\csdef{mkpairedouter#1delim}##1{%
    \begingroup
      \blx@blxinit
      \blx@setsfcodes
      \csuse{outer@#1@delim@open}##1\csuse{outer@#1@delim@close}%
    \endgroup}}

\blx@createmkpairedouterdelim{cite}
\blx@createmkpairedouterdelim{parencite}
\blx@createmkpairedouterdelim{textcite}

