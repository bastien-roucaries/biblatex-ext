\ProvidesFile{ext-biblatex-aux.def}
  [2018/02/15 v-1 auxiliaries for extended biblatex citation style (MW)]

\def\extblx@requiredbiblatexversion{3.11}
\def\extblx@requiredbiblatexdate{2018/02/10}

\@ifpackagelater{biblatex}{\extblx@requiredbiblatexdate}
  {}
  {\PackageWarningNoLine{extbiblatex}
     {Outdated version of biblatex detected.\MessageBreak
      Upgrade to biblatex \extblx@requiredbiblatexversion\space
      (\extblx@requiredbiblatexdate) or later.\MessageBreak
      I found '\csuse{abx@version} (\csuse{abx@date})'}}

%{<context>}{<opening delim>}{<closing delim>}
\newcommand*{\DeclarePairedCiteOuterDelim}{%
  \extblx@declarepaireddelim{outer}}
\newrobustcmd*{\DeclarePairedCiteInnerDelim}{%
  \extblx@declarepaireddelim{inner}}

%{<inner/outer>}{<cite context>}{<opening delim>}{<closing delim>}
\def\extblx@declarepaireddelim#1#2#3#4{%
  \csdef{extblx@delim@#2@#1@open}{#3}%
  \csdef{extblx@delim@#2@#1@close}{#4}}

%{<alias>}{<cite context>}
\newcommand*{\DeclarePairedCiteOuterDelimAlias}{%
  \@ifstar
    {\extblx@declarepaireddelim@alias@i{outer}}
    {\extblx@declarepaireddelim@alias{outer}}}
\newcommand*{\DeclarePairedCiteInnerDelimAlias}{%
  \@ifstar
    {\extblx@declarepaireddelim@alias@i{inner}}
    {\extblx@declarepaireddelim@alias{inner}}}

%{<inner/outer>}{<alias>}{<cite context>}
\def\extblx@declarepaireddelim@alias#1#2#3{%
  \csdef{extblx@delim@#2@#1@open}{\csuse{extblx@delim@#3@#1@open}}%
  \csdef{extblx@delim@#2@#1@close}{\csuse{extblx@delim@#3@#1@close}}}
\def\extblx@declarepaireddelim@alias@i#1#2#3{%
  \csletcs{extblx@delim@#2@#1@open}{extblx@delim@#3@#1@open}%
  \csletcs{extblx@delim@#2@#1@close}{extblx@delim@#3@#1@close}}

% declare \mkpaired...delim commands
\newrobustcmd*{\extblx@create@mkpaireddelim}[2]{%
  \protected\csdef{mkpaired#1#2delim}##1{%
    \begingroup
      \blx@blxinit
      \blx@setsfcodes
      \csuse{extblx@delim@#2@#1@open}%
      ##1%
      \csuse{extblx@delim@#2@#1@close}%
    \endgroup}}

\extblx@create@mkpaireddelim{outer}{cite}
\extblx@create@mkpaireddelim{outer}{parencite}
\extblx@create@mkpaireddelim{outer}{textcite}
\extblx@create@mkpaireddelim{outer}{bibxrefcite}

\extblx@create@mkpaireddelim{inner}{cite}
\extblx@create@mkpaireddelim{inner}{parencite}
\extblx@create@mkpaireddelim{inner}{textcite}
\extblx@create@mkpaireddelim{inner}{footcite}

\endinput
